{
  "name": "products-react-express-mongodb",
  "version": "1.0.0",
  "description": "Products App implemented using React/Redux front-end, Express back-end and MongoDB storage.",
  "main": "index.js",
  "repository": "https://github.com/vcholak/products-react-express-mongodb.git",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "express",
    "mongodb",
    "es6",
    "babel",
    "boostrap4",
    "sass",
    "webpack"
  ],
  "scripts": {
    "build": "webpack",
    "dev": "webpack serve",
    "dev2": "nodemon -w src/server --exec \"babel-node src/server --presets=env,react --plugins=transform-class-properties\"",
    "build2": "babel src/server -s -D -d server --presets=env,react --plugins=transform-class-properties",
    "start": "node dist/server.js",
    "prestart": "yarn run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.2.0",
    "eslint": "^7.31.0",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.12",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@popperjs/core": "^2.9.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^5.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jquery": "^3.6.0",
    "mongodb": "^4.0.1",
    "morgan": "^1.10.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modal": "^3.14.3",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "resource-router-middleware": "^0.7.0",
    "serve-favicon": "^2.5.0",
    "shutdown-handler": "^1.0.1",
    "validator": "^13.6.0"
  }
}
